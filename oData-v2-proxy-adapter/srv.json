{
  "definitions": {
    "CatalogService": {
      "kind": "service",
      "@path": "browse"
    },
    "CatalogService.Authors": {
      "kind": "entity",
      "query": {
        "SELECT": {
          "from": {
            "ref": [
              "my.bookshop.Authors"
            ]
          }
        }
      },
      "elements": {
        "ID": {
          "key": true,
          "type": "cds.UUID"
        },
        "name": {
          "type": "cds.String",
          "length": 111
        },
        "dateOfBirth": {
          "type": "cds.Date"
        },
        "dateOfDeath": {
          "type": "cds.Date"
        },
        "placeOfBirth": {
          "type": "cds.String"
        },
        "placeOfDeath": {
          "type": "cds.String"
        },
        "books": {
          "type": "cds.Association",
          "cardinality": {
            "max": "*"
          },
          "target": "CatalogService.Books",
          "on": [
            {
              "ref": [
                "books",
                "author"
              ]
            },
            "=",
            {
              "ref": [
                "$self"
              ]
            }
          ]
        }
      },
      "$syntax": "projection"
    },
    "CatalogService.Books": {
      "kind": "entity",
